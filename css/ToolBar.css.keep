    padding: 0px;
    margin: 0px;
    height: inherit;
}
div.searchDiv
{
    float: right;
    border-width: 1px;
    border-style: solid;
    border-radius: 20px 20px 20px 20px;
    border-color: #BEBEBE;
    background-color: white;
    width: 200px;
    height: 22px;
    margin-right: 2px;
    position:relative;
    
    
    box-shadow: 0px 1px 3px #000;
    -moz-box-shadow: 0px 1px 3px #000;
    -webkit-box-shadow: 0px 1px 3px #000;
    
    behavior: url(/Pie/PIE.htc);
}

input.inputSearch
{
