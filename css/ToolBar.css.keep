/* CSS Document */


ul.toolbar
{
    position: static;
    width: 100%;
    height: 28px;
    margin: 0px;
    padding: 0px;
    background-color: inherit;
    list-style-type: none;

    box-shadow: 0px 1px 5px #000;
    -moz-box-shadow: 1px 0px 5px #000;
    -webkit-box-shadow: 1px 0px 5px #000;
    
    behavior: url(/Pie/PIE.htc);
}

ul.toolbar li
{
    display: inline;
}



ul.toolbar li a
{
    top: 0px;
    outline: none;
    float: left;
    cursor: pointer;
    width: 28px;
    height: 23px;
    
    opacity: 0.65;
    /*-ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=30)";*/
    filter: alpha(opacity=65);
    -moz-opacity: 0.65;
    -khtml-opacity: 0.65;
      
    box-shadow: 0px 3px 5px #000;
    -moz-box-shadow: 0px 3px 5px #000;
    -webkit-box-shadow: 0px 3px 5px #000;
    
    border-radius:4px;
    -moz-border-radius:4px;
    -webkit-border-radius:4px;    
    
 }


ul.toolbar li a.enabled:hover
{
    -ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=100)";
    filter: alpha(opacity=100) !important;
    -moz-opacity: 1.0;
    -khtml-opacity: 1.0;
    opacity: 1.0;

}

ul.toolbar li a.enabled:active
{
    outline:none;
    position:relative;
    top: 3px;
    box-shadow: none;
    -moz-box-shadow: none;
    -webkit-box-shadow: none;
}

.enabled
{
    position:relative;
    behavior: url(/Pie/PIE.htc);
    
}
    /* prevent IE9 using PIE */
:root *> #exampleTransparency {
   behavior: none;    
}

.disabled
{
    opacity: 0.1 !important;
    -ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=10)";
    filter: gray alpha(opacity=30) url("data:image/svg+xml;utf8,<svg xmlns=\'http://www.w3.org/2000/svg\'><filter id=\'grayscale\'><feColorMatrix type=\'matrix\' values=\'0.3333 0.3333 0.3333 0 0 0.3333 0.3333 0.3333 0 0 0.3333 0.3333 0.3333 0 0 0 0 0 1 0\'/></filter></svg>#grayscale") !important;
    -moz-opacity: 0.1;
    -khtml-opacity: 0.1;
    -webkit-filter: grayscale(100%) !important;
}

